<%-include('../partials/header.ejs')%>
<div class="container">
    <h1>This is the Timeline</h1>
    <% if(user){ %>
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal">Create New Post</button>
        <%-include('./newpost.ejs')%>
    <%}%>
    <br>
    <%if(posts.length>0){
        posts.forEach(post=>{ %>
            <%-include('./posttile.ejs',{post})%>
        <%})%>
    <%}%>

    <p>Forum will be displayed here</p>
</div>
